<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>page-1.jsp</welcome-file>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <context-param>
        <param-name>IDE</param-name>
        <param-value>ECLIPSE</param-value>
    </context-param>
    <servlet>
        <servlet-name>BasicServlet</servlet-name>
        <servlet-class>servlets.BasicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GoalKeeperServlet</servlet-name>
        <servlet-class>servlets.GoalKeeperServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BasicServlet</servlet-name>
        <url-pattern>/basicServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GoalKeeperServlet</servlet-name>
        <url-pattern>/goalKeeper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
</web-app>